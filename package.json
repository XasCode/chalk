{
	"name": "@xascode/chalk",
	"version": "0.1.1",
	"description": "Terminal string styling done right",
	"license": "MIT",
	"repository": "XasCode/chalk",
	"funding": "https://github.com/chalk/chalk?sponsor=1",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"type": "module",
	"imports": {
		"#ansi-styles": "./source/vendor/ansi-styles/index.js",
		"#supports-color": "./source/vendor/supports-color/index.js"
	},
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"types": "./dist/source/index.d.ts",
	"engines": {
		"node": "^12.17.0 || ^14.13 || >=16.0.0"
	},
	"scripts": {
		"bench": "matcha benchmark.js",
		"build": "rm -rf dist && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && vite build && npm run build:cjs && npm run build:mjs",
		"build:cjs": "echo { \"type\": \"commonjs\" } > dist/cjs/package.json",
		"build:mjs": "echo { \"type\": \"module\" } > dist/esm/package.json",
		"test": "vitest run --coverage --reporter dot"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"color",
		"colour",
		"colors",
		"terminal",
		"console",
		"cli",
		"string",
		"ansi",
		"style",
		"styles",
		"tty",
		"formatting",
		"rgb",
		"256",
		"shell",
		"xterm",
		"log",
		"logging",
		"command-line",
		"text"
	],
	"devDependencies": {
		"@types/node": "^16.11.10",
		"@vitest/coverage-istanbul": "^0.33.0",
		"ava": "^5.3.1",
		"c8": "^7.10.0",
		"color-convert": "^2.0.1",
		"execa": "^6.0.0",
		"log-update": "^5.0.0",
		"matcha": "^0.7.0",
		"tsd": "^0.19.0",
		"vitest": "^0.33.0",
		"xo": "^0.53.0",
		"yoctodelay": "^2.0.0"
	},
	"sideEffects": false,
	"xo": {
		"rules": {
			"unicorn/prefer-string-slice": "off",
			"@typescript-eslint/consistent-type-imports": "off",
			"@typescript-eslint/consistent-type-exports": "off",
			"@typescript-eslint/consistent-type-definitions": "off",
			"unicorn/expiring-todo-comments": "off"
		}
	},
	"c8": {
		"reporter": [
			"text",
			"lcov"
		],
		"exclude": [
			"source/vendor"
		]
	},
	"dependencies": {
		"@types/lodash": "^4.14.195",
		"builtin-modules": "^3.3.0",
		"lodash": "^4.17.21",
		"vite": "^4.3.9",
		"vite-plugin-commonjs-externals": "^0.1.3",
		"vite-plugin-dts": "^3.1.0"
	}
}
